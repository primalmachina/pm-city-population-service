{
  "name": "pm-city-population-service",
  "version": "0.1.0",
  "type": "module",
  "description": "nodejs api with fastify for a population service",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run pre-build && node src/index.js",
    "start-watch": "npm run pre-build && nodemon src/index.js",
    "dev": "nodemon src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pre-build": "sh downloadPopulationData.sh"
  },
  "keywords": [],
  "author": "Zach Fortier",
  "license": "ISC",
  "dependencies": {
    "async-exit-hook": "^2.0.1",
    "async-mutex": "^0.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.2",
    "fastify-cors": "^6.1.0",
    "fastify-helmet": "^7.1.0",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "ghooks": "^2.0.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "lint-staged"
    }
  }
}
